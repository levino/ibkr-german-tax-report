    "url": "https://github.com/levino/ibkr-german-tax-report/issues"
  },
  "homepage": "https://github.com/levino/ibkr-german-tax-report#readme",
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.113",
    "@biomejs/biome": "^2.2.4",
    "@types/node": "^24.4.0",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
